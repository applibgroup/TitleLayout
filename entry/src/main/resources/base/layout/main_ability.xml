<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    xmlns:dante="http://schemas.huawei.com/res/dante"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical"

    >

    <com.siberiadante.titlelayout.TitleBarLayout
        ohos:id="$+id:title_layout_one"
        ohos:height="match_content"
        ohos:width="match_parent"
        dante:d_is_back_view="true"
        dante:d_is_immersive_state_bar="false"
        dante:d_left_image="$media:icon_back_black"
        dante:d_left_image_padding_end="0vp"
        dante:d_left_image_padding_start="10vp"
        dante:d_left_image_width="30vp"
        dante:d_left_text="back"
        dante:d_left_text_color="#000000"
        dante:d_left_text_padding_start="0vp"
        dante:d_left_text_size="45fp"
        dante:d_left_text_style="normal"
        dante:d_line_height="10px"
        dante:d_right_text="report"
        dante:d_right_text_size="45fp"
        dante:d_right_text_padding_start="0vp"
        dante:d_right_text_padding_end="20vp"
        dante:d_right_text_style="normal"
        dante:d_title_color="#000000"
        dante:d_title_layout_background="#FA0303"
        dante:d_title_layout_height="70vp"
        dante:d_title_size="70fp"
        dante:d_title_style="normal"
        dante:d_title_text="Here is the title"/>


    <com.siberiadante.titlelayout.TitleBarLayout
        ohos:id="$+id:title_layout_two"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:top_margin="30vp"
        dante:d_is_back_view="false"
        dante:d_left_text="test"
        dante:d_left_text_color="#000000"
        dante:d_left_text_padding_start="20vp"
        dante:d_left_text_size="44fp"
        dante:d_left_text_style="bold"
        dante:d_line_height="10px"
        dante:d_right_text="submit"
        dante:d_right_text_color="#000000"
        dante:d_right_text_size="44fp"
        dante:d_right_text_style="bold"
        dante:d_right_text_padding_start="0vp"
        dante:d_right_text_padding_end="20vp"
        dante:d_title_color="#000000"
        dante:d_title_layout_background="#FA0303"
        dante:d_title_layout_height="70vp"
        dante:d_title_size="60fp"
        dante:d_title_style="bold"
        dante:d_title_text="Here is the title"/>

    <com.siberiadante.titlelayout.TitleBarLayout
        ohos:id="$+id:title_layout_three"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:top_margin="30vp"
        dante:d_is_back_view="true"
        dante:d_left_image="$media:icon_back_black"
        dante:d_left_image_padding_start="10vp"
        dante:d_left_image_width="30vp"
        dante:d_line_height="5px"
        dante:d_right_image="$media:icon_collection"
        dante:d_right_image_width="30vp"
        dante:d_right_image_padding_end="15vp"
        dante:d_title_color="#000000"
        dante:d_title_layout_background="#FA0303"
        dante:d_title_layout_height="70vp"
        dante:d_title_size="60fp"
        dante:d_title_text="Here is the title"/>

    <com.siberiadante.titlelayout.TitleBarLayout
        ohos:id="$+id:title_layout_four"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:top_margin="30vp"
        dante:d_is_back_view="false"
        dante:d_left_image="$media:icon_back_black"
        dante:d_left_image_padding_start="10vp"
        dante:d_left_image_width="30vp"
        dante:d_line_height="5px"
        dante:d_right_image="$media:icon_collection"
        dante:d_right_image_width="30vp"
        dante:d_right_image_padding_end="15vp"
        dante:d_subtitle_size="35vp"
        dante:d_subtitle_text="subtitle"
        dante:d_title_color="#000000"
        dante:d_title_layout_background="#FA0303"
        dante:d_title_layout_height="70vp"
        dante:d_title_size="50vp"
        dante:d_title_text="Here is the title" />

</DirectionalLayout>
